
@{
    ViewData["Title"] = "LoginClient";
    Layout = "~/Views/Shared/LayoutClient.cshtml";
}
<div class="breadbg">
    <div class="container">
        <div class="row">
            <ul class="breadcrumb">
                <li><a href="index9328.html?route=common/home"><i class="fa fa-home"></i></a></li>
                <li><a href="indexe223.html?route=account/account">Account</a></li>
                <li><a href="@Url.Action("Login","User")">Login</a></li>
            </ul>
        </div>
    </div>
</div>
<div id="account-login" class="container acpage">
    <div class="row">
        <div id="content" class="col-sm-8 col-md-9 col-xs-12 colright">

            <div class="row">
                <div class="col-sm-12 col-md-6 col-lg-6 col-xs-12">
                    <div class="well">
                        <h1>New Customer</h1>
                        <p><strong>Register Account</strong></p>
                        <p>By creating an account you will be able to shop faster, be up to date on an order's status, and keep track of the orders you have previously made.</p>
                        <a href="index5502.html?route=account/register" class="btn btn-primary">Continue</a>
                    </div>
                </div>
                <div class="col-sm-12 col-md-6 col-lg-6 col-xs-12">
                    <div class="well">
                        <h1>Returning Customer</h1>
                        <p><strong>I am a returning customer</strong></p>
                        <form action="https://opencart.dostguru.com/FD04/upbasket_01/index.php?route=account/login" method="post" enctype="multipart/form-data">
                            <div class="form-group">
                                <label class="control-label" for="input-email">E-Mail Address</label>
                                <input type="text" name="email" value="" placeholder="E-Mail Address" id="input-email" class="form-control" />
                            </div>
                            <div class="form-group">
                                <label class="control-label" for="input-password">Password</label>
                                <input type="password" name="password" value="" placeholder="Password" id="input-password" class="form-control" />
                                <div class="text-right"><a href="indexacda.html?route=account/forgotten">Forgotten Password</a></div>
                            </div>
                            <input type="submit" value="Login" class="btn btn-primary" />
                            <input type="hidden" name="redirect" value="https://opencart.dostguru.com/FD04/upbasket_01/index.php?route=account/wishlist" />
                            <a class="btn btn-primary" href="@Url.Action("GoogleLogin", "User")">Log In With Google</a>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <aside id="column-right" class="col-sm-4 col-md-3 col-xs-12 hidden-xs">
            <div class="list-group accolumn">


                <h3><svg class="" width="20px" height="20px"> <use xlink:href="#acluser"></use> </svg>ACCOUNT SETTINGS</h3>
                <a href="indexe223.html?route=account/login" class="list-group-item">Login</a> <a href="index5502.html?route=account/register" class="list-group-item">Register</a> <a href="indexacda.html?route=account/forgotten" class="list-group-item">Forgotten Password</a>
                <a href="indexe223.html?route=account/account" class="list-group-item">My Account</a>

            </div>
        </aside>
    </div>
</div>



